{
  "expo": {
    "name": "textApp",
    "slug": "textApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "resizeMode": "contain",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "android": {
        "fadeOutDuration": 1000
      },
      "ios": {
        "fadeOutDuration": 1000
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "package": "com.psdinesh.textApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "resizeMode": "contain"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "6a84efd4-d91c-4fff-bd6f-47407a8ce209"
      }
    }
  }
}
