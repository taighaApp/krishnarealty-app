{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx":  "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/*": [ "./*" ],
      "@types/*": ["types/type.d.ts"],
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "rules": {
  "import/no-unresolved": "error"
}
}
